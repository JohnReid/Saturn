  pages = {861--873},
  file = {Full Text PDF:/home/john/.mozilla/firefox/tpi3ht8v.default/zotero/storage/3RU2WMST/Jolma et al. - 2010 - Multiplexed massively parallel SELEX for character.pdf:application/pdf;Snapshot:/home/john/.mozilla/firefox/tpi3ht8v.default/zotero/storage/RGCGUE9T/861.html:text/html},
  eprinttype = {pmid},
  eprint = {20378718}
}

