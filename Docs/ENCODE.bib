  urldate = {2016-10-14},
  date = {2016-04-01},
  pages = {D110--D115},
  file = {Mathelier et al_2016_JASPAR 2016.pdf:/home/john/.mozilla/firefox/tpi3ht8v.default/zotero/storage/D9CWW72N/Mathelier et al_2016_JASPAR 2016.pdf:application/pdf;Snapshot:/home/john/.mozilla/firefox/tpi3ht8v.default/zotero/storage/UJEJD397/D110.html:text/html},
