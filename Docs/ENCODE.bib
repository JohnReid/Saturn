  shortjournal = {Nat Meth},
  author = {He, Housheng Hansen and Meyer, Clifford A. and Hu, Sheng'en Shawn and Chen, Mei-Wei and Zang, Chongzhi and Liu, Yin and Rao, Prakash K. and Fei, Teng and Xu, Han and Long, Henry and Liu, X. Shirley and Brown, Myles},
  urldate = {2016-08-06},
  date = {2014-01},
  pages = {73--78},
  keywords = {_tablet,Chromatin analysis,DNA sequencing,gene regulation,gene regulatory networks},
  file = {He et al_2014_Refined DNase-seq protocol and data analysis reveals intrinsic bias in.pdf:/home/john/.mozilla/firefox/tpi3ht8v.default/zotero/storage/RWT5IG46/He et al_2014_Refined DNase-seq protocol and data analysis reveals intrinsic bias in.pdf:application/pdf;Snapshot:/home/john/.mozilla/firefox/tpi3ht8v.default/zotero/storage/88CXXJIN/nmeth.2762.html:text/html}
